pipeline {
    agent any

    stages {
         stage('Clonar repositório') {
            steps {
                git branch: 'main', url: 'https://github.com/JoaoVictor531/testes-api-cy.git'
            }
        }
        stage('Instalar dependências') {
            steps {
               sh 'npm install'
            }
        }
         stage('Executar teste') {
            steps {
                sh 'NO_COLOR=1 npm run cy:run-cy | true npm run cy:run-cy'
            }
        }
    }
}