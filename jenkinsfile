pipeline {
    agent any

    stages {
        stage('Clonar repositório') {
            steps {
                git branch: 'aula-ci', url: 'https://github.com/JoaoVictor531/testes-api-cy.git'            }
        }
        stage('Instalar dependências'){
            steps {
                bat 'npm install'
            }
        }
        stage('Executar teste'){
            steps {
                bat 'npm run cy:run-ci'
            }
        }
    }

}